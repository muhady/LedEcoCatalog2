  "ConnectionStrings":
  {
    "KatalogDbConnectionString": "Server=.;Database=LEDeco_Katalog;Trusted_Connection=True;MultipleActiveResultSets=True"
  },
  "Logging":
  {
    "LogLevel":
    {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Serilog":
  {
    "MinimumLevel":
    {
      "Default": "Debug",
      "Override":
      {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo":
    [
      {
        "Name": "Async",
        "Args":
        {
          "configure":
          [
            {
              "Name": "File",
              "Args":
              {
                "path": "Logs/LedEcoKatalog..log",
                "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] [{RemoteIpAddress}] {Message:lj}{NewLine}{Exception}",
                "rollingInterval": "Day",
                "retainedFileCountLimit": 7,
                "buffered": false
              }
            }
          ]
        }
      }
    ],
    "Enrich":
    [
      "FromLogContext"
    ]
  }
}
